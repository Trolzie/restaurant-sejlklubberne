<ul>
    {% set navPages = collections.all | eleventyNavigation %}
    {%- for entry in navPages %}
        {% if entry.title == 'Restaurant' %}
            <li>
                <a href="{{ entry.url | url }}">{{ entry.title }}</a>
            </li>
        {% endif %}
        {% if entry.title == 'Takeaway' %}
            <li>
                <a href="{{ entry.url | url }}">{{ entry.title }}</a>
            </li>
        {% endif %}
        {% if entry.title == 'Arrangement' %}
            <li>
                <a href="{{ entry.url | url }}">{{ entry.title }}</a>
            </li>
        {% endif %}
    {%- endfor %}
</ul>
