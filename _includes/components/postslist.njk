<ul class="event-list">
	{% for post in postslist | reverse %}
		<li{% if post.url == url %} data-current="current item"{% endif %}>
			<h3>
				<a href="{{ post.url | url }}">
					{% if post.data.title %}
						{{ post.data.title }}
					{% else %}
						Untitled
					{% endif %}
				</a>
			</h3>
		</li>
	{% endfor %}
</ul class="event-list">
